snippet unwrap "unwrap a variable"
var $1;
`!p
snip.rv = t[1].split(',')[0].strip()` = $2[0]; `!p
words = t[1].split(',')
snip.rv = " ".join(['%s = %s[%d];'%(words[i].strip(), t[2], i) for i in range(1, len(words))])
`
endsnippet

snippet print "Console log"
console.log($1);$0
endsnippet

snippet bindfn "create a anonymous bind function"
(function($2){$0}).bind($1:this)
endsnippet

snippet func "new function"
function($1){
    $0
}
endsnippet
